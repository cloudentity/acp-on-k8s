          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "select \n  rpu.created_at AS \"DATE\",\n  u.tenant_user_id as \"User Id\",\n  a.tenant_app_id as \"Application ID\", \n  rpu.overall_score::NUMERIC * 100 as LOA,\n(\n    SELECT \n      CONCAT(\n        '{',\n        STRING_AGG(\n          CONCAT(\n            substring(analyzer_key,9), \n            ':', ROUND((analyzer_data->'data'->>'score')::NUMERIC, 2)\n          ), \n          ', '\n        ),\n        '}'\n      )\n    FROM LATERAL json_each(rpu.metadata) AS domains(domain_key, domain_value),\n         LATERAL json_each(domain_value->'data'->'analyzers') AS analyzers(analyzer_key, analyzer_data)\n  ) AS \"Analyzer Scores\",\n  Upper(rpu.auth_method) as \"Auth Method\", \n  rpu.ip_address as \"IP\", \n  Upper(rpu.country) as \"Country\",\n  Upper(rpu.state) as \"State\", \n  Upper(rpu.city) as \"City\", \n  Upper(rpu.auth_status) as \"Auth Status\", \n  rpu.dbfp_fingerprint as \"Fingerprint\",\n  rpu.transaction_id as \"Transaction Id\",\n  rpu.trace_id as \"OTEL Trace Id\"\nfrom risk_process_unit rpu\ninner join organizations as o on o.id = rpu.organization_id\ninner join applications as a on a.id = rpu.application_id\ninner join tenants as t on t.id = rpu.tenant_id\ninner join users as u on u.id = rpu.user_id\nwhere \nUpper(t.name) In($Tenant) AND\nUpper(o.tenant_org_id) IN($Org) AND\nrpu.auth_status is not null and\n$__timeFilter(rpu.updated_at) \norder by rpu.updated_at desc limit 100",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
