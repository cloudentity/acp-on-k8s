          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH daily_counts AS (\n  SELECT \n    queued_at::date AS train_date,\n    handler,\n    COUNT(*) AS number_trained\n  FROM training_params\n  WHERE is_model_acceptable IS NOT False\n  GROUP BY train_date, handler\n),\nsummary AS (\n  SELECT\n    handler,\n\n    -- 2-day average: from (today - 2) to (today - 1)\n    (\n      SELECT AVG(number_trained)\n      FROM daily_counts d2\n      WHERE d2.handler = d.handler\n        AND d2.train_date >= CURRENT_DATE - INTERVAL '2 days'\n        AND d2.train_date < CURRENT_DATE\n    ) AS avg_last_2_days,\n\n    -- 10-day average: from (today - 10) to (today - 1)\n    (\n      SELECT AVG(number_trained)\n      FROM daily_counts d10\n      WHERE d10.handler = d.handler\n        AND d10.train_date >= CURRENT_DATE - INTERVAL '10 days'\n        AND d10.train_date < CURRENT_DATE\n    ) AS avg_last_10_days\n\n  FROM daily_counts d\n  WHERE d.train_date >= CURRENT_DATE - INTERVAL '10 days'\n    AND d.train_date < CURRENT_DATE\n  GROUP BY handler\n)\nSELECT *\nFROM summary\nORDER BY handler;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "2-Day vs 10-Day Training Average (Per Handler)",
